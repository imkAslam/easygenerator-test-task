# security
JWT_SECRET = your secret
JWT_EXPIRES_IN = 8h
API_SECURITY_AUTH = auth

# swagger
SWAGGER_ENABLE = true
SWAGGER_PATH = docs
SWAGGER_VERSION = 1.0.0



DB_LOGGING = ["error"]

#Database
MONGO_URI= your mongodb uri
