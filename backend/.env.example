PORT = your port
NODE_ENV = development

APP_NAME =Test Task
APP_HOST = http://localhost:${PORT}
FRONTEND_URL = https://domainhost.com
APP_LOCALE = en
TIMEZONE = UTC
GLOBAL_PREFIX = api/

# logger
LOGGER_LEVEL = verbose
LOGGER_MAX_FILES = 31

